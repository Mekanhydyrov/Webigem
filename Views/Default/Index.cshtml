
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<!doctype html>
<html lang="tr">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="theme-color" content="#2c2c2c">
    <meta name="msapplication-navbutton-color" content="#2c2c2c">
    <meta name="apple-mobile-web-app-status-bar-style" content="#2c2c2c">
    <title>Web Yazılımı webigem | WEB Tasarım | Ankara Yazılım Şirketi | Webigem Yazılım Şirketi | Ankara | İstanbul | İzmir</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <meta name="robots" content="index, follow" />
    <meta name="keywords" content="web yazılım,yazilim,web tasarım,grafik tasarım,e-ticaret,SEO,web site onarımı,site bakımı" />
    <meta name="description" content="Ankara Yazilim Sirketi,Yazilim şirketi, Webigem Yazılım Şirketi | Web Yazılım | Web Tasarım | E-Ticaret | Grafik Tasarım | Ankara - İstanbul-İzmir" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1" name="viewport" />
    <meta name="publisher" content="webigem.com" />
    <meta name="copyright" content="webigem ®" />
    <meta name="date-creation-yyyy" content="2021" />
    <meta name="classification" content="Machine" />
    <meta name="author" content="webigem Software Company E-commerce website construction & Service 7/24 Opening" />
    <meta name="rating" content="all" />
    <meta name="expires" content="no" />
    <meta name="language" content="TR" />
    <meta name="distribution" content="global" />
    <meta name="rating" content="all" />
    <meta name="googlebot" content="NOODP" />
    <meta http-equiv="cache-control" content="no-cache" />
    <meta http-equiv="pragma" content="no-cache" />
    <meta name="revisit-after" content="1 Day" />
    <meta name="apple-mobile-web-app-capable" content="yes" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="GENERATOR" content="webigem.com" />
    <meta name="RESOURCE-TYPE" content="DOCUMENT" />
    <meta name="DISTRIBUTION" content="GLOBAL" />
    <meta name="ROBOTS" content="INDEX, FOLLOW" />
    <meta name="REVISIT-AFTER" content="1 DAYS" />
    <meta name="RATING" content="GENERAL" />
    <meta http-equiv="PAGE-ENTER" content="RevealTrans(Duration=0,Transition=1)" />
    <meta property="og:type" content="website" />
    <meta property="og:site_name" content="webigem Software Company E-commerce website construction & Service 7/24 Opening" />
    <meta property="og:url" content="https://webigem.com/" />
    <meta property="og:title" content="webigem Software Company E-commerce website construction & Service 7/24 opening" />
    <meta property="og:description" content="webigem Software Company E-commerce website construction & Service 7/24 opening" />
    <meta property="og:image" content="~/Resimler/WEBİGEM.png" />
    <link rel="canonical" href="https://webigem.com/" />


    @if (TempData["iletisimmesaj"] != null)
    {
        <link href="~/Content/Alert/package/dist/sweetalert2.min.css" rel="stylesheet" />
        <script src="~/Content/Alert/package/dist/sweetalert2.min.js"></script>
        <script>
            Swal.fire("webigem.com", "Mesajınız gönderilmiştir!", 'success');
        </script>
    }

    @if (TempData["formgonder"] != null)
    {
        <link href="~/Content/Alert/package/dist/sweetalert2.min.css" rel="stylesheet" />
        <script src="~/Content/Alert/package/dist/sweetalert2.min.js"></script>
        <script>
            Swal.fire("webigem.com", "Talebiniz tarafımıza iletilmiştir. En kısa sürede web danışmanlarımız tarafından talebinize cevap verilecektir.", 'success');
        </script>
    }
</head>
<body>


    <div class="banner-area">
        <div class="container">
            <div class="banner-into">
                <div class="row align-items-center">
                    <div class="col-lg-7">
                        <div class="banner-content">
                            <h1>
                                İşinizi İki Kat Daha Hızlı Artırın
                            </h1>
                            <p>
                               WEBIGEM olarak; özel yazılım geliştirme, kurumsal web tasarım ve
                                mobil uygulama geliştirme alanlarında siz değerli müşterilerimize profesyonel destek ve çözümler sunuyoruz.
                            </p>
                            <div class="banner-content-btn">
                                <a href="~/default/TumHizmetler" class="learn-btn">Hizmetlerimize Göz Atın</a>
                                <div class="banner-play-btn">
                                    <a href="#" class="play-btn">
                                        <i class='bx bx-play'></i> Video İzle
                                    </a>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-5">
                        <div class="banner-img-2">
                            <img src="~/Content/WebigemDesign/main/default/assets/img/home-2/1.png" alt="Banner Images">
                            <div class="dots">
                                <img src="~/Content/WebigemDesign/main/default/assets/img/home-2/dots.png" alt="Dots Images">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="home-shape">
            <div class="shape1">
                <img src="~/Content/WebigemDesign/main/default/assets/img/shape/1.png" alt="shape">
            </div>
            <div class="shape2">
                <img src="~/Content/WebigemDesign/main/default/assets/img/shape/2.png" alt="shape">
            </div>
            <div class="shape3">
                <img src="~/Content/WebigemDesign/main/default/assets/img/shape/3.png" alt="shape">
            </div>
            <div class="shape4">
                <img src="~/Content/WebigemDesign/main/default/assets/img/shape/4.png" alt="shape">
            </div>
            <div class="shape5">
                <img src="~/Content/WebigemDesign/main/default/assets/img/shape/5.png" alt="shape">
            </div>
            <div class="shape6">
                <img src="~/Content/WebigemDesign/main/default/assets/img/shape/6.png" alt="shape">
            </div>
            <div class="shape7">
                <img src="~/Content/WebigemDesign/main/default/assets/img/shape/3.png" alt="shape">
            </div>
        </div>
    </div>


    <div class="submission-top">
        <div class="container">
            <form class="submission-form box-shadow">
                <div class="row">
                    <div class="col-lg-5 col-md-6">
                        <div class="form-group">
                            <input type="text" class="form-control" required="" id="website" placeholder="Sitenizi İnceleyelim (URL)">
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-6">
                        <div class="form-group border-line">
                            <input type="text" class="form-control" required="" id="email" placeholder="E Posta">
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-3 offset-md-5 offset-lg-0">
                        <button type="submit" class="default-btn">Gönder</button>
                    </div>
                </div>
            </form>
        </div>
    </div>


    <!--Hakkımızda Üst Kısım-->

    <div class="about-area pt-100 pb-70">
        <div class="container-fluid">
            <div class="row align-items-center">
                <div class="col-lg-5 col-xxl-6 pl-0">
                    <div class="about-img-2">
                        <div class="about-man">
                            <img src="~/Content/WebigemDesign/main/default/assets/img/about/2.png" alt="About Images">
                        </div>
                        <div class="about-shape1">
                            <img src="~/Content/WebigemDesign/main/default/assets/img/about/s-1.png" alt="Shape">
                        </div>
                        <div class="about-shape2">
                            <img src="~/Content/WebigemDesign/main/default/assets/img/about/s-2.png" alt="Shape">
                        </div>
                    </div>
                </div>
                <div class="col-lg-7 col-xxl-6">
                    <div class="about-content about-width">
                        <span>Hakkımızda</span>
                        @Html.Action("HakkimizdaPart1", "Default")


                    </div>
                </div>
            </div>
        </div>
        <div class="about-shape-2">
            <img src="~/Content/WebigemDesign/main/default/assets/img/shape/about-shape1.png" alt="Shape">
        </div>
    </div>

    <!--Hizmetler-->
    <section class="service-area pb-70">
        <div class="container">
            <div class="section-title text-center">
                <span>
                    Servisimiz
                </span>
                <h2>En Hızlı ve Eşsiz İş Büyütme Hizmeti Sunuyoruz</h2>
                <p>
                    Bir okuyucunun, sayfa düzenine bakarken sayfanın okunabilir
                    içeriği tarafından dikkatinin dağılacağı uzun süredir bilinen bir gerçektir.
                </p>
            </div>
            <div class="service-slider owl-carousel owl-theme pt-45">

                @Html.Action("AnaHizmet", "Default")



            </div>
        </div>
    </section>



    <!--Neden bizi Seçmelisiniz-->

    <div class="faq-bg">
        <div class="faq-bg-into pt-100 pb-70">
            <div class="container">
                <div class="row align-items-center">
                    <div class="col-lg-6">
                        <div class="faq-content">
                            <span>Neden Bizi Seçmelisiniz?</span>
                            <h2>
                                En İyi ve Özel Yazılım Şirketiyiz
                            </h2>
                            <p>
                                Web tasarım, yazılım çözümleri, reklamlar ve SEO gibi
                                dijital dünyanın gerekleri olan çalışmalarda referansları
                                ile kendini ispatlamış olan WEBIGEM ekibi olarak; tüm dijital talepler için çözüm sunmaktayız.
                            </p>
                        </div>
                        <div class="faq-accordion">
                            <ul class="accordion">


                                @Html.Action("ProjeCalismaSureci", "Default")
                            </ul>
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="faq-img-2">
                            <img src="~/Content/WebigemDesign/main/default/assets/img/faq/2.jpg" alt="fAQ Images">
                        </div>
                        <div class="faq-content-text">

                            @Html.Action("HakkimizdaPart", "Default")


                            <div class="row">
                                @Html.Action("TammlananPSayisi2", "Default")


                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>


    <!--Portfoil-->

    <section class="portfolio-area portfolio-mt pt-100 pb-70">
        <div class="container">
                <div class="section-title text-center">
                    <span>Portföyümüz</span>
                    <h2>
                        Onlarca Firmayla İş Yaptık
                    </h2>
                    <p>
                        Müşteri memnuniyetini en önemli ilkemiz olarak benimseyerek çalışma yaptığımız müşteriler ile geliştirme, 
                        bakım ve destek süreçlerinde güven ve memnuniyet ilişkisini sürdürerek kalitemizden taviz vermiyoruz.
                    </p>
                </div>
                <div class="portfolio-slider pt-45 owl-carousel owl-theme">
                    @Html.Action("ProjelerPart", "Default")

                    

                </div>
            </div>
    </section>


    <div class="counter-area-2 pt-100 pb-70">
        <div class="container">
            <div class="row">
                @Html.Action("TammlananPSayisi", "Default")

            </div>
        </div>
    </div>


    <div class="price-area price-bg pt-100 pb-70">

    </div>




    <!--Müşteri Yorumları-->
    <div class="client-area pb-70">
        <div class="container-fluid p-0">
            <div class="section-title text-center">
                <span>Referans</span>
                <h2>Müşterilerimizin Görüşleri</h2>
                <p>
                    Bir okuyucunun, sayfa düzenine bakarken sayfanın okunabilir
                    içeriği tarafından dikkatinin dağılacağı uzun süredir bilinen bir gerçektir.
                </p>
            </div>
            <div class="client-slider owl-carousel owl-theme">
                @Html.Action("MMusteriYorumu", "Default")


            </div>
        </div>
    </div>


    <div class="blog-area pb-70">
        <div class="container">
            <div class="section-title text-center">
                <span>Bloglar</span>
                <h2>Yeni Postlar</h2>
                <p>
                    It is a long established fact that a reader will be
                    distracted by the readable content of a page when looking at its layout.
                </p>
            </div>
            <div class="row pt-45">

                @Html.Action("OneCikanBlok1", "Default")


            </div>
        </div>
    </div>

    <!--Bizi Takip Edin-->
    @*<div class="subscribe-area ptb-100">
        <div class="container">
            <div class="subscribe-content">
                <h2>Subscribe Bonsa for Daily Update</h2>
                <form class="subscribe-form">
                    <input type="email" class="form-control" placeholder="Enter your email">
                    <button class="submit-btn" type="submit">
                        Subscribe
                    </button>
                </form>
            </div>
        </div>
    </div>*@
</body>
</html>
