<body>


    <div class="inner-banner inner-bg8">
        <div class="container">
            <div class="inner-title text-center">
                <h3>Bizim Takım</h3>
                <ul>
                    <li>
                        <a href="/">Ana Sayfa</a>
                    </li>
                    <li>
                        <i class='bx bxs-chevrons-right'></i>
                    </li>
                    <li>Takım</li>
                </ul>
            </div>
        </div>
    </div>


    <div class="member-area pt-100 pb-70">
        <div class="container">
            <div class="section-title text-center">
                <span>Uzman Takımımız</span>
                <h2>
                    Webigem'in Büyük Başarısının Arkasında Rol Yapıyorlar
                </h2>
                <p>
                    Bir okuyucunun, sayfa düzenine bakarken sayfanın
                    okunabilir içeriği tarafından dikkatinin
                    dağılacağı uzun süredir bilinen bir gerçektir.
                </p>
            </div>
            <div class="row pt-45">
                @foreach (var x in Model)
                {
                    <div class="col-lg-3 col-sm-6">
                        <div class="member-card">
                            <div class="member-img">
                                <a href="#">
                                    <img src="@x.Resim" alt="Member Images">
                                </a>
                                <div class="member-content">
                                    <a href="#">
                                        <h3>@x.AdSoyad</h3>
                                    </a>
                                    <span>@x.Meslek</span>
                                    <div class="social-icon">
                                        <ul>
                                            <li>
                                                <a href="@x.Face" target="_blank">
                                                    <i class='bx bxl-linkedin'></i>
                                                </a>
                                            </li>
                                            <li>
                                                <a href="@x.Tweet" target="_blank">
                                                    <i class='bx bxl-github'></i>
                                                </a>
                                            </li>
                                            <li>
                                                <a href="@x.inst" target="_blank">
                                                    <i class='bx bxl-instagram'></i>
                                                </a>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                }
            </div>
        </div>
    </div>
</body>
