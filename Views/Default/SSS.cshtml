    <div class="faq-area-bg pt-100 pb-70">
        <div class="container">
            <div class="section-title text-center">
                <span>SSS</span>
                <h2>
                    Sıkça Sorulan Sorular
                </h2>
                <p>
                    Bir okuyucunun, sayfa düzenine bakarken sayfanın okunabilir
                    içeriği tarafından dikkatinin dağılacağı uzun süredir bilinen bir gerçektir.
                </p>
            </div>
           
            <div class="row align-items-center pt-45">
                @foreach (var x in Model)
                {
                    <div class="col-lg-12">
                        <div class="faq-accordion faq-accordion-width">
                            <ul class="accordion">
                                <li class="accordion-item">
                                    <a class="accordion-title" href="javascript:void(0)">
                                        <i class='bx bx-chevron-down'></i>
                                        @x.Baslik
                                    </a>
                                    <div class="accordion-content">
                                        <p>
                                            @x.icerik
                                        </p>
                                    </div>
                                </li>
                            </ul>
                        </div>
                    </div>
                }
                </div>
           
         

        </div>
    </div>


    


